{
  "name": "budgeto_v2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "lint": "eslint --config ./eslint.config.js \"{src,public,cypress,scripts,.react-router}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "react-router dev",
    "start:network": "react-router dev --host 0.0.0.0",
    "start:prod": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:init": "playwright install --with-deps"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.2.1",
    "@react-router/node": "^7.10.1",
    "@react-router/serve": "^7.10.1",
    "isbot": "^5.1.31",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.10.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.10.1",
    "@storybook/react-vite": "^9.1.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "happy-dom": "^14.12.3",
    "playwright": "^1.48.0",
    "sass": "^1.94.0",
    "typescript": "^5.9.2",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
